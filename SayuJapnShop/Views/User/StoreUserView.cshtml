@{
}
@model SayuJapnShop.Models.User

<div class="m-2">
    <p class="text-center" style="font-size:25px">User</p>
    <table class="table table-hover" style="border-radius:5px;box-shadow:0 2px 2px 2px grey">
        <thead>
            <tr>
                <th>No</th>
                <th>UserName</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Role</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>1</td>
                <td>@Model.UserName</td>
                <td>@Model.Email</td>
                <td>@Model.PhoneNumber</td>
                <td>@Model.Role</td>

            </tr>
        </tbody>
    </table>
</div>



